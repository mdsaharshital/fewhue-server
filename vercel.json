{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/auth/(.*)", "dest": "/api/auth/$1" },
    { "src": "/api/product/(.*)", "dest": "/api/product/$1" },
    { "src": "/api/category/(.*)", "dest": "/api/category/$1" },
    { "src": "/api/order/(.*)", "dest": "/api/order/$1" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
