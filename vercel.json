{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/.*",
      "dest": "/index.js"
    },
    {
      "src": "/product/.*",
      "dest": "/index.js"
    },
    {
      "src": "/category/.*",
      "dest": "/index.js"
    },
    {
      "src": "/order/.*",
      "dest": "/index.js"
    }
  ]
}
